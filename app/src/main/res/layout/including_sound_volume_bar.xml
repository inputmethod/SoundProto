<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/iv_sound_volume_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:contentDescription="@string/sound_volume_bar_low"
        android:src="@drawable/sound_volume_low"/>

    <com.typany.ui.skinui.custom.fixcrash.FixCrashSeekBar
        android:id="@+id/seekbar"
        android:layout_width="match_parent"
        android:layout_height="60dip"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp"
        android:layout_centerVertical="true"
        android:layout_gravity="center"
        android:layout_toEndOf="@id/iv_sound_volume_low"
        android:layout_toRightOf="@id/iv_sound_volume_low"
        android:layout_toStartOf="@+id/iv_sound_volume_up"
        android:layout_toLeftOf="@+id/iv_sound_volume_up"
        />
